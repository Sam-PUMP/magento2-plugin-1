<?xml version="1.0"?>

<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Event/etc/events.xsd">
    <event name="catalog_entity_attribute_set_save_after">
        <observer name="sb_catalog_entity_attribute_save_after" instance="Springbot\Main\Observer\AttributeSetSaveAfterObserver" />
    </event>
    <event name="catalog_entity_attribute_delete_after">
